package com.springcore.constructorInjection;

public class Person {
private String name;
private int id;
public Person() {
	super();
	// TODO Auto-generated constructor stub
}
public Person(String name, int id) {
	super();
	this.name = name;
	this.id = id;
}
@Override
public String toString() {
	return "Person [name=" + name + ", id=" + id + "]";
}

}
